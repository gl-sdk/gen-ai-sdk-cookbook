# GL AIP API Configuration
# Copy this file to .env and fill in your values

# Required: AIP API URL (e.g., https://aip.obrol.id/)
AIP_API_URL=
AIP_API_KEY=


# Arxiv MCP authentication (optional - MCP won't be added if not set)
ARXIV_MCP_API_KEY=your-arxiv-api-key
ARXIV_MCP_AUTH_TOKEN=your-bearer-token
